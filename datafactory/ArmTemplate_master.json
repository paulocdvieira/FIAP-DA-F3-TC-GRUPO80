{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "fiap-f3-tc-pnad"
        },
        "fiap_input_dl_ls_accountKey": {
            "type": "secureString",
            "metadata": "Secure string for 'accountKey' of 'fiap_input_dl_ls'"
        },
        "fiap_out_sqldb_ls_password": {
            "type": "secureString",
            "metadata": "Secure string for 'password' of 'fiap_out_sqldb_ls'"
        },
        "fiap_input_dl_ls_properties_typeProperties_url": {
            "type": "string",
            "defaultValue": "https://fiapf3tcdl.dfs.core.windows.net/"
        },
        "fiap_out_sqldb_ls_properties_typeProperties_server": {
            "type": "string",
            "defaultValue": "fiapf3tc-srv.database.windows.net"
        },
        "fiap_out_sqldb_ls_properties_typeProperties_database": {
            "type": "string",
            "defaultValue": "fiapf3tc-db"
        },
        "fiap_out_sqldb_ls_properties_typeProperties_userName": {
            "type": "string",
            "defaultValue": "adm"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "fiap-f3-tc-pnad_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2024-03-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "fiap_input_dl_ls_accountKey": {
                        "value": "[parameters('fiap_input_dl_ls_accountKey')]"
                    },
                    "fiap_out_sqldb_ls_password": {
                        "value": "[parameters('fiap_out_sqldb_ls_password')]"
                    },
                    "fiap_input_dl_ls_properties_typeProperties_url": {
                        "value": "[parameters('fiap_input_dl_ls_properties_typeProperties_url')]"
                    },
                    "fiap_out_sqldb_ls_properties_typeProperties_server": {
                        "value": "[parameters('fiap_out_sqldb_ls_properties_typeProperties_server')]"
                    },
                    "fiap_out_sqldb_ls_properties_typeProperties_database": {
                        "value": "[parameters('fiap_out_sqldb_ls_properties_typeProperties_database')]"
                    },
                    "fiap_out_sqldb_ls_properties_typeProperties_userName": {
                        "value": "[parameters('fiap_out_sqldb_ls_properties_typeProperties_userName')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}